<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="spring.teamproject.dabang.domain.account.AccountRepository">
<select id="getAccount" parameterType="Integer" 
	resultType="spring.teamproject.dabang.domain.account.Account">
	SELECT
		user_code,
		nName
	FROM
		user_mst
	where
		user_code = #{usercode}
	</select>	

	
</mapper>













